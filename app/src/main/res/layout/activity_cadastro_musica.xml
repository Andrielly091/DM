<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".cadastro.CadastroMusica">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Senha:"
        android:textColor="#1EA0C0"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="593dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="E-mail:"
        android:textColor="#1EA0C0"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="495dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome:"
        android:textColor="#1EA0C0"
        android:textSize="20sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="379dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="385dp"
        android:layout_height="270dp"
        app:srcCompat="@drawable/logoMusica"
        tools:layout_editor_absoluteX="97dp"
        tools:layout_editor_absoluteY="2dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="417dp"
        android:layout_height="56dp"
        android:text="Por favor, insira seus                                 dados para se cadastrar!"
        android:textColor="#1EA0C0"
        android:textSize="24sp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="128dp"
        tools:layout_editor_absoluteY="281dp" />

    <EditText
        android:id="@+id/editTextTextPersonName"
        android:layout_width="356dp"
        android:layout_height="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="79dp"
        tools:layout_editor_absoluteY="418dp" />

    <EditText
        android:id="@+id/editTextTextEmailAddress2"
        android:layout_width="364dp"
        android:layout_height="44dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="538dp" />

    <EditText
        android:id="@+id/editTextTextPassword2"
        android:layout_width="362dp"
        android:layout_height="52dp"
        android:ems="10"
        android:inputType="textPassword"
        tools:layout_editor_absoluteX="76dp"
        tools:layout_editor_absoluteY="635dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="193dp"
        android:layout_height="53dp"
        android:backgroundTint="#00C2CB"
        android:text="CADASTRAR"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="158dp"
        tools:layout_editor_absoluteY="732dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="133dp"
        android:layout_height="45dp"
        android:backgroundTint="#1EA0C0"
        android:text="VOLTAR"
        tools:layout_editor_absoluteX="188dp"
        tools:layout_editor_absoluteY="813dp" />
</androidx.constraintlayout.widget.ConstraintLayout>